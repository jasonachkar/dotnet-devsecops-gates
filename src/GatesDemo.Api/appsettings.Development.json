{
  // ==========================================================================
  // Development-only Logging Overrides
  // ==========================================================================
  // More verbose logging in development for debugging.
  // These settings are ONLY applied when ASPNETCORE_ENVIRONMENT=Development.
  // Production/CI uses the base appsettings.json logging levels.
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information"
    }
  }
}
